<server description="Hello World Liberty server">

  <!-- To access this server from a remote client add a host attribute to the following
    element, e.g. host="*" -->
  <httpEndpoint host="*" httpPort="${default.http.port}" httpsPort="${default.https.port}"
    id="defaultHttpEndpoint" />

  <enterpriseApplication id="EnterpriseHelloWorld.ear"
    location="EnterpriseHelloWorld.ear"
    name="EnterpriseHelloWorld.ear" />
</server>
